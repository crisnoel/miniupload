<div class="jumbotron">
  <h1 ALIGN=center>Listing Uploads</h1><br>
  <p ALIGN=center><a class="btn btn-lg btn-success" href="/uploads/new" role="button">New Upload</a></p>
  <br>
  <table align="center" width="60%" height="100%">
  <thead>
    <tr>
      <th align="center" valign="center"><p class="lead" ALIGN=center>FileName</p></th>
      <th align="center" valign="center"><p class="lead" ALIGN=center>Description</p></th>
      <th align="center" valign="center"><p class="lead" ALIGN=center>Tags</p></th>
    </tr>
  </thead>
  <tbody>
    <% @uploads.each do |upload| %>
      <tr>
        <td><%= upload.fileName %></td>
        <td><%= upload.description %></td>
        <td><%= upload.tags %></td>
        <!--<td><%= upload.user %></td>-->
        <td><%= link_to 'Show', upload %></td>
        <td><%= link_to 'Edit', edit_upload_path(upload) %></td>
        <td><%= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>